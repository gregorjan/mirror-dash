{
  "name": "dashboard",
  "version": "0.1.0",
  "description": "preact dashboard",
  "author": "me",
  "license": "MIT",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "ebuild": "webpack --mode production && electron-builder --dir"
  },
  "build": {
    "appId": "dashboard",
    "files": [
      "electron.js",
      "dist/**/*"
